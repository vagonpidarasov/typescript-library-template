{
  "name": "typescript-library-template",
  "description": "Provide your description",
  "version": "0.0.0",
  "sideEffects": false,
  "main": "main/index.js",
  "module": "module/index.js",
  "types": "module/index.d.ts",
  "files": [
    "main/**/*",
    "module/**/*",
    "styles/**/*",
    "CHANGELOG*.md",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf module main styles",
    "build:styles": "cpy --parents 'src/**/*.scss' styles",
    "build:es": "./babel/babel.sh ./babel/babel.config.es.js module",
    "build:cjs": "./babel/babel.sh ./babel/babel.config.cjs.js main",
    "build:types": "tsc --project tsconfig-build.json --outDir ./module --emitDeclarationOnly true --declaration true --noEmit false",
    "build:watch:es": "npm run build:es -- --watch",
    "build:watch:cjs": "npm run build:cjs -- --watch",
    "build:watch:types": "npm run build:types -- --watch",
    "build:watch:styles": "npm run build:styles && chokidar \"src/**/*.scss\" -c \"npm run build:styles\"",
    "dist": "npm-run-all clean --parallel build:*",
    "dist:watch": "run-p build:watch:*",
    "start": "npm run dist:watch",
    "typecheck": "tsc",
    "prepare": "husky install",
    "test": "jest --config ./jest/jest.config.js",
    "tdd": "npm run test -- --watch",
    "prepublishOnly": "npm run dist",
    "linter": "eslint --ext .js,.ts,.tsx './' && echo 'All matched files use Linter code style!'",
    "prettier": "prettier --check './**/*.{js,ts,tsx}'",
    "format:js:eslint:staged": "eslint --fix",
    "format:js:prettier:staged": "prettier --write"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "npm run format:js:prettier:staged",
      "npm run format:js:eslint:staged"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vagonpidarasov/typescript-library-template.git"
  },
  "keywords": [],
  "author": "Stanislav Beresnev",
  "license": "UNLICENSED",
  "dependencies": {
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/eslint-parser": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chokidar-cli": "^2.1.0",
    "core-js": "^2.6.12",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-testing-library": "^4.2.0",
    "husky": "7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.5.1",
    "lint-staged": "11.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "core-js": "^2.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "importSort": {
    ".js": {
      "style": "module-compact"
    }
  }
}
